{
  "type": "object",
  "required": [
    "id",
    "defaultBranch",
    "engine",
    "owner",
    "repository",
    "s3ServiceName",
    "awsBucketName",
    "viewLink",
    "demoViewLink",
    "previewLink",
    "basicAuth",
    "createdAt",
    "isActive"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "demoBranch": {
      "type": "string"
    },
    "demoDomain": {
      "type": "string"
    },
    "defaultConfig": {
      "type": "string"
    },
    "defaultBranch": {
      "type": "string"
    },
    "domain": {
      "type": "string"
    },
    "engine": {
      "type": "string",
      "enum": ["jekyll", "hugo", "static"]
    },
    "owner": {
      "type": "string"
    },
    "previewConfig": {
      "type": "string"
    },
    "demoConfig": {
      "type": "string"
    },
    "publishedAt": {
      "type": "string",
      "format": "date-time"
    },
    "repoLastVerified": {
      "type": "string",
      "format": "date-time"
    },
    "repository": {
      "type": "string"
    },
    "s3ServiceName": {
      "type": "string"
    },
    "awsBucketName": {
      "type": "string"
    },
    "viewLink": {
      "type": "string"
    },
    "demoViewLink": {
      "type": "string"
    },
    "previewLink": {
      "type": "string"
    },
    "basicAuth": {
      "type": "object",
      "properties": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        }
      }
    },
    "isActive": {
      "type": "boolean"
    },
    "containerConfig": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "size": {
          "type": "string"
        }
      }
    },
    "users": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "username", "email"],
        "not": {
          "required": ["githubAccessToken", "githubUserId"]
        }
      }
    },
    "canEditLiveUrl": {
      "type": "boolean"
    },
    "canEditDemoUrl": {
      "type": "boolean"
    }
  }
}
